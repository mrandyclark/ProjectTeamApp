doctype html
html
	head
		title= title
		script(type='text/javascript', src='/vendor/jquery-1.10.2.min.js')
		script(type='text/javascript', src='/vendor/bootstrap/js/bootstrap.min.js')
		script(type='text/javascript', src='/vendor/f2.min.js')
		block scripts

		link(rel='stylesheet', href="/vendor/bootstrap/css/bootstrap.min.css")
		link(rel='stylesheet', href="/vendor/bootstrap/css/bootstrap-theme.min.css")
		link(rel='stylesheet', href="/stylesheets/main.css")
		block styles

	body
		div(class='container-fluid')
			div(class='row')
				div(class='col-md-12')
					h1(class='h2') Project Team Dashboard
					nav(id='nav' role='navigation')
						ul(class='nav nav-pills')
							li
								a(href='/') Home

							li(role='presentation', class='dropdown')
								a(class='dropdown-toggle', data-toggle='dropdown', href='#', role='button' aria-expanded='false') Team Settings 
									span(class='caret')

								ul(class='dropdown-menu', role='menu')
									li(class='active')
										a(href='/members') Team Members
									li
										a Project Calendar Dates
									li
										a Clients
									li
										a Projects
									li
										a Code Reviews
							li
								a Upcoming Projects
							li
								a Current Schedule
							li
								a Code Reviews

		main(class='container-fluid')
			block content
